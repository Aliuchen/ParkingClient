<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable  name="inputview">
        <!--图片-->
        <attr name="input_icon" format="reference"></attr>
        <!--字体-->
        <attr name="input_hint" format="string"></attr>
        <!--是否密文-->
        <attr name="is_pass" format="boolean"></attr>
    </declare-styleable>


    <declare-styleable  name="nav">
        <!--图片-->
        <attr name="icon" format="reference"></attr>
        <!--字体-->
        <attr name="hint" format="string|dimension"></attr>
        <!--具体信息-->
        <attr name="info" format="string|dimension"></attr>
        <!--是否密文-->
        <attr name="pass" format="boolean"></attr>
        <attr name="right_icon" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="SeatTableView">
        <attr name="seat_checked" format="reference" />
        <attr name="seat_sold" format="reference" />
        <attr name="seat_available" format="reference" />
        <attr name="overview_checked" format="color" />
        <attr name="overview_sold" format="color" />
        <attr name="txt_color" format="color"/>
    </declare-styleable>
</resources>